
(def S (array -6 77 0.0 30.6 -15))


(fn-call or S)

(fn-call and S)


(defn negative-of (n)
    (- 0 n)
)

(fn-map negative-of S)


(defn is-positive (n)
    (> n 0)
)

(array-filter S is-positive)


(def values (array 4.5 2))
(def operators (array + - * /))

(defn fn-call-oper (oper)
    (fn-call oper values)
)

(fn-map fn-call-oper operators)

