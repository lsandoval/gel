
(def S (array -6 77 0.0 30.6 -15))


(apply or S)

(apply and S)


(defn negative-of (n)
    (- 0 n)
)

(map negative-of S)


(defn is-positive (n)
    (> n 0)
)

(array-filter S is-positive)


(def values (array 4.5 2))
(def operators (array + - * /))

(defn operate-values (oper)
    (apply oper values)
)

(map operate-values operators)


(def units (array 1 2 3))
(def tens (10 20 30))

(map array units tens)

(apply + (map * units tens))

