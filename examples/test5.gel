
(def colors (array "red" "green" "blue"))

(defn palette ()
    (var colors)
)

(array-append! (palette) "white")
(print colors)

(array-set! (palette) 3 "black")
(print colors)

(set! (palette) (array "cyan" "magenta" "yellow"))
(print colors)


