
(function ! not (test)
    (if (eval test)
        FALSE
        TRUE
    )
)

(function ! when (test & body)
    (if (eval test)
        (apply do body)
        FALSE
    )
)

(function ! unless (test & body)
    (if (eval test)
        TRUE
        (apply do body)
    )
)


(not 0)

(not 1)

(define n 5)

(when (= n 5)
    (print "Positive test of 'when'")
)

(when (not (= n 5))
    (print "Negative test of 'when'")
)

(unless (= n 5)
    (print "Negative test of 'unless'")
)

(unless (not (= n 5))
    (print "Positive test of 'unless'")
)

