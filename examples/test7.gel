
(defn f (a b c)
    (print "a =" a)
    (print "b =" b)
    (print "c =" c)
)


(defn fn-call-by-name (f ks)
    (def h (fn-call hash ks))
    (fn-call f
        (fn-map
            (fn (k) (hash-get h k))
            (fn-args f)
        )
    )
)

(fn-name f)

(fn-code f)

(fn-args f)

(fn-call-by-name f ("c" 30 "a" 10 "b" 20))

