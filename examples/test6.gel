
(def fundamentals
    (hash
        ("e" 2.718281828459045)
        ("pi" 3.141592653589793)
        ("c" 299792458)
    )
)

(print fundamentals)

(hash-size fundamentals)

(hash-set! fundamentals "g" 9.822)

(print fundamentals)

(hash-remove! fundamentals "c")

(hash-get fundamentals "pi")

(hash-keys fundamentals)

(def (hash-values h)
    (map
        (closure (k)
            (hash-get h k)
        )
        (hash-keys h)
    )
)

(hash-values fundamentals)


(def romans
    (hash
        (1000 "M")
        (900 "CM")
        (500 "D")
        (400 "CD")
        (100 "C")
        (90 "XC")
        (50 "L")
        (40 "XL")
        (10 "X")
        (9 "XX")
        (5 "V")
        (4 "IV")
        (1 "I")
    )
)

(hash-keys romans)

(apply +
    (map
        (closure (i) (hash-get romans i))
        (array 1000 900 50 10 10 5 1 1)
    )
)


