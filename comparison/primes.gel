
(define primes (array 2))

(for n (range 3 1000000)
    (for i primes
        (if (= (% n i) 0)
            (break)
        )
        (if (>= (* i i) n)
            (begin
                (append primes n)
                (break)
            )
        )
    )
)

(display "Calculated " (length primes) " primes\n")

(quit)

